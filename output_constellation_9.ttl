ns1:Book7447b3c8-6f81-4326-b29a-dc9da9690a00 a ns1:Book ;
    ns1:author "David Walliams" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46295" ;
