ns1:Bookf4fa7201-8e41-442f-bd59-e3b491e747ff a ns1:Book ;
    ns1:author "Anton Tchekhov" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=26238" ;
