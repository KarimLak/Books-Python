ns1:Bookb7e0fe4a-3732-4610-bc81-ff4cc95b481a a ns1:Book ;
    ns1:author "Florence Thinard" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49288" ;
