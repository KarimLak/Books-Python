ns1:Bookdd1a60c3-9c1d-4977-9d7a-475ee315eb20 a ns1:Book ;
    ns1:author "Tomi Ungerer" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=14735" ;
