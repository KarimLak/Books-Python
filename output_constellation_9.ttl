ns1:Book57d36aec-eebc-4780-8748-58786fab2595 a ns1:Book ;
    ns1:author "Catharina Valckx" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45377" ;
