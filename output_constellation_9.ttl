ns1:Book434c2359-d69d-474b-9045-1e4a01f03a3d a ns1:Book ;
    ns1:author "Tute" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45725" ;
