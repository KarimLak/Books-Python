ns1:Bookb49157c9-3b9f-4504-b33c-31f46235324d a ns1:Book ;
    ns1:author "Louise Tondreau-Levert" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=36617" ;
