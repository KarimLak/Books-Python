ns1:Book14dbbeb1-a182-4282-82e3-d384cac475de a ns1:Book ;
    ns1:author "Chisato Tashiro" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=41406" ;
