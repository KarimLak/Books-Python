ns1:Book559336b0-de70-4f75-bc63-498133626dfc a ns1:Book ;
    ns1:author "Chris Van Allsburg" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47958" ;
