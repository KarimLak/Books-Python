ns1:Book71dab850-8544-4625-bf98-42cd64561f24 a ns1:Book ;
    ns1:author "Jeanne Willis" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=42592" ;
