ns1:Bookdec4acb9-d75d-4eae-ae63-0a94800cb2d6 a ns1:Book ;
    ns1:author "Richard Walker" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=34425" ;
    ns1:datePublished "2005"^^xsd:gYear ;
