ns1:Booke04bf42e-4730-4f55-9468-f829380853b6 a ns1:Book ;
    ns1:author "Scott Westerfeld" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=43335" ;
