ns1:Booke0523946-ab9a-4b39-84a2-1c36081a7eb6 a ns1:Book ;
    ns1:author "Camille Von Rosenschild" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49144" ;
