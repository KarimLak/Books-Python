ns1:Book3a6049f2-bbfd-4711-841a-a4ec4c82f5b2 a ns1:Book ;
    ns1:author "Osvaldo Torres" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50003" ;
