ns1:Bookd08cd776-1b14-459c-a0b6-f69f24982c49 a ns1:Book ;
    ns1:author "Leo Timmers" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50503" ;
    ns1:datePublished "2015"^^xsd:gYear ;
