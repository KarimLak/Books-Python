ns1:Book0aa81f8a-03bb-461a-b1b1-882611757d92 a ns1:Book ;
    ns1:author "Mary Wallace" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=41275" ;
    ns1:datePublished "2009"^^xsd:gYear ;
