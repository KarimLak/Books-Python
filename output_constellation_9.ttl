ns1:Book0a948662-b61c-4ed1-a299-60a5586c30cb a ns1:Book ;
    ns1:author "Sarah Angela et Khan Wilkes" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=44395" ;
