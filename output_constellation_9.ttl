ns1:Book8b8eb46e-be76-4581-8bb2-59dda0ddfe36 a ns1:Book ;
    ns1:author "Drew Weing" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50039" ;
