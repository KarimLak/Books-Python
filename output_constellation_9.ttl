ns1:Bookfd292340-62f9-4242-8a59-13664543e5b5 a ns1:Book ;
    ns1:author "Gabrielle Vincent" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=39310" ;
