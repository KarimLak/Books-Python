ns1:Bookf8746efa-4530-4d2e-ae78-2226655f2e28 a ns1:Book ;
    ns1:author "José Mauro de Vasconcelos" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48378" ;
