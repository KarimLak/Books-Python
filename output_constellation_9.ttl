ns1:Book5b723499-d859-42b5-a12e-204fe951118c a ns1:Book ;
    ns1:author "Scott Westerfeld" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45266" ;
    ns1:datePublished "2010"^^xsd:gYear ;
