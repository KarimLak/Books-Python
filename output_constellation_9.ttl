ns1:Book7ce1942d-7b5e-481a-bfdb-5367428afeb9 a ns1:Book ;
    ns1:author "Mélanie Watt" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=37642" ;
    ns1:datePublished "2007"^^xsd:gYear ;
