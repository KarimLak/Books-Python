ns1:Book59541f1e-ec59-4cb0-9db1-d469daa8186a a ns1:Book ;
    ns1:author "Shaun Tan" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48267" ;
