ns1:Book9b19668c-02f2-464d-83ea-a5f2122b5131 a ns1:Book ;
    ns1:author "Mario Urbanet" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=41846" ;
