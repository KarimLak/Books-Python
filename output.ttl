    ns1:isbn "ISBN 9782919300037" ;
    ns1:author "Patel, Shenaz (1966-....). Auteur du texte | Pelon, Sébastien (1976-....). Auteur du texte | " ;
    ns1:publisher "Atelier des Nomades (Boulogne-Billancourt)" ;
    ns1:datePublished "2014"^^xsd:gYear ;
    ns1:title "Contes de l'Île Maurice / Shenaz Patel, Sébastien Pelon" ;
    ns1:description "Code à barres commercial : EAN 9782919300037" ;
