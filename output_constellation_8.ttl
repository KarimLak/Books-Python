ns1:Bookfd78a336-8c4c-41ad-b31e-9536eb52d202 a ns1:Book ;
    ns1:author "Raman Prinja" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50506" ;
