ns1:Bookb6a55716-d210-4929-8c78-9ce34a1c1895 a ns1:Book ;
    ns1:author "Mario Ramos" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45198" ;
