ns1:Bookac65937d-7080-40ce-893c-66f2093eeca3 a ns1:Book ;
    ns1:author "Maryse Pelletier" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=26576" ;
    ns1:datePublished "2004"^^xsd:gYear ;
