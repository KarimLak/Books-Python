ns1:Booke98b48ae-7218-4f82-8f6f-ec2c07d50b20 a ns1:Book ;
    ns1:author "Mario Ramos" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=34616" ;
