ns1:Book4b8cb082-a2b5-4570-8933-2e8bbeb96b07 a ns1:Book ;
    ns1:author "Michel Piquemal" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49099" ;
    ns1:datePublished "2010"^^xsd:gYear ;
