ns1:Booka80229bd-31fb-4f79-be49-f911ca75bb37 a ns1:Book ;
    ns1:author "Odile Robert" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=44139" ;
