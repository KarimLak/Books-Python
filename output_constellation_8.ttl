ns1:Book01c0fe80-efaa-4781-851d-caa87f7ec297 a ns1:Book ;
    ns1:author "Jacob et Wilhelm Renate d'après Grimm Raecke" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47850" ;
