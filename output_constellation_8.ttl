ns1:Book4733e67e-638b-46a8-ae6b-419ec8c03762 a ns1:Book ;
    ns1:author "Alain Raimbault" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46481" ;
    ns1:datePublished "2012"^^xsd:gYear ;
