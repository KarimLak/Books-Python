ns1:Bookacc92525-ad43-4b62-ad26-5efbc957aca4 a ns1:Book ;
    ns1:author "Professeur Génius" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=35512" ;
    ns1:datePublished "2006"^^xsd:gYear ;
