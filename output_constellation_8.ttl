ns1:Bookdea2921f-7dbd-442d-8f52-fd6c8e5502e9 a ns1:Book ;
    ns1:author "Martine Podesto" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=14437" ;
