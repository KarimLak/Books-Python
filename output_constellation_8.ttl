ns1:Book4249a2b7-1b65-4008-91b0-5b3cdeb26641 a ns1:Book ;
    ns1:author "Anne Pearson" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45371" ;
