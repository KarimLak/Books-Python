ns1:Bookf0732674-7843-44f6-839c-db293b035425 a ns1:Book ;
    ns1:author "Philip Pullman" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=44660" ;
    ns1:datePublished "1999"^^xsd:gYear ;
