ns1:Book5539a7a7-82da-48a7-b21e-afaf97aca2c5 a ns1:Book ;
    ns1:author "Florence Pinaud" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47971" ;
    ns1:datePublished "2013"^^xsd:gYear ;
