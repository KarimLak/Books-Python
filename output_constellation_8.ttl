ns1:Book22f8b574-b645-4a07-9b50-17cc240868e6 a ns1:Book ;
    ns1:author "Lucie Papineau" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50108" ;
