ns1:Book7595c1f5-aa09-4f8a-ab3e-edf9c2ca4bf5 a ns1:Book ;
    ns1:author "Mario Ramos" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46997" ;
