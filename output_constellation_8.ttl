ns1:Book239466f0-2b81-40fd-bbd1-0ba0d6ba7963 a ns1:Book ;
    ns1:author "Marie Roberge" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47370" ;
