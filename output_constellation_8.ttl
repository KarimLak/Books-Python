ns1:Book06164c79-e9fa-44e7-a77b-9f5edc280e0c a ns1:Book ;
    ns1:author "Halfdan Rasmussen" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=37981" ;
