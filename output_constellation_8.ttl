ns1:Bookf1303972-c3d6-4c37-8cd8-0c04a5105b27 a ns1:Book ;
    ns1:author "Michel Piquemal" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=43056" ;
