ns1:Bookc6c0e073-0589-458a-b0a8-fc7c1c679046 a ns1:Book ;
    ns1:author "Laure Cédric et Salès Ray" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48578" ;
