ns1:Book1c93d637-b937-41bb-9a67-38105efc8f3f a ns1:Book ;
    ns1:author "Richard Platt" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=36432" ;
