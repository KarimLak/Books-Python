ns1:Bookf8ce97a0-3fa5-406f-b7f1-3b49a6c492d4 a ns1:Book ;
    ns1:author "Sylvie Roberge" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45015" ;
    ns1:datePublished "2010"^^xsd:gYear ;
