ns1:Bookfea26aaf-d1c2-468e-8078-2179dde11b00 a ns1:Book ;
    ns1:author "Jean E. Pendziwol" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47851" ;
