ns1:Book372644ad-a0c1-46a0-b4eb-94fd35e09d70 a ns1:Book ;
    ns1:author "Sylvie Poillevé" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=38897" ;
    ns1:datePublished "2003"^^xsd:gYear ;
