ns1:Book8d1454ca-0deb-4b83-a645-576acadb3dcb a ns1:Book ;
    ns1:author "Tony Ross" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=40932" ;
