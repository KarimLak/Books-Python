ns1:Book1eed3b84-126b-4a2b-a180-b47f91f5cc4d a ns1:Book ;
    ns1:author "Angela Royston" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50182" ;
