ns1:Book9c7b1cbc-67f1-463a-b6be-1da87a4ede44 a ns1:Book ;
    ns1:author "Martine Noël-Maw" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48401" ;
