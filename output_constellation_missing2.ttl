ns1:Book9a81c383-5865-4794-82de-f28b7279b068 a ns1:Book ;
    ns1:author "Alain Serres" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=43543" ;
