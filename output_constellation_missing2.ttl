ns1:Bookce81339a-26f7-4dd5-a0b0-9be71c93a245 a ns1:Book ;
    ns1:author "Katherine Rundell" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50132" ;
