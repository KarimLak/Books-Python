ns1:Book60864964-d232-49f1-85d7-70924f299bdc a ns1:Book ;
    ns1:author "Dominique de Saint Mars" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=10015" ;
