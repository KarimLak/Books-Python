ns1:Bookbcc27823-7f81-45fb-b782-deeaa5da277b a ns1:Book ;
    ns1:author "David Shannon" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=18885" ;
