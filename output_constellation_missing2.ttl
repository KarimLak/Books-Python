ns1:Bookfc4c5c25-ff6a-4918-a7b1-a1e425bfd539 a ns1:Book ;
    ns1:author "Ian Ogilvy" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=39342" ;
    ns1:datePublished "2004"^^xsd:gYear ;
