ns1:Booke98859d6-58c4-44dd-b828-f6d32df157e8 a ns1:Book ;
    ns1:author "Robert Louis Maxime d'après Stevenson Rovere" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49363" ;
