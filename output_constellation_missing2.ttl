ns1:Book776b264e-e53f-4ef0-9733-b50198b6b267 a ns1:Book ;
    ns1:author "Claude Roy" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=41741" ;
