ns1:Bookf94ad2a8-512a-4dc8-a556-516708958c57 a ns1:Book ;
    ns1:author "Brian Selznick" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51038" ;
