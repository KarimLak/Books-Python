ns1:Bookcfb1b829-f8f9-49a2-9a67-f297ad3771b8 a ns1:Book ;
    ns1:author "Allen Say" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46733" ;
