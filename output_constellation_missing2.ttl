ns1:Booke2b0bb91-85cf-4c46-833f-2bc220fdb85a a ns1:Book ;
    ns1:author "Dominique Roques" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46617" ;
