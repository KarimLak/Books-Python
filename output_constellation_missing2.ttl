ns1:Book2dc48f63-81d8-4490-aaf2-9c17ee91dfae a ns1:Book ;
    ns1:author "Philip C. Stead" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49497" ;
