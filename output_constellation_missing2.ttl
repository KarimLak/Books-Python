ns1:Book475caa54-8c87-4546-a81d-039b4afcba9d a ns1:Book ;
    ns1:author "Robert D. San Souci" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=37793" ;
    ns1:datePublished "2000"^^xsd:gYear ;
