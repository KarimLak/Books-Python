ns1:Bookfd281c2c-2151-4488-8e54-7db4b7c8286b a ns1:Book ;
    ns1:author "Lane Smith" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46219" ;
