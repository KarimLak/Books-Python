ns1:Book0c8e1758-3af5-431a-be0b-c7485e316cbd a ns1:Book ;
    ns1:author "Karine Safa" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=36792" ;
