ns1:Book187703cc-6ee9-454a-9851-548f269cf084 a ns1:Book ;
    ns1:author "Paul T. Somerville" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46490" ;
