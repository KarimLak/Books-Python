ns1:Bookd49ab82f-4423-4580-9da3-0dd55e772fcf a ns1:Book ;
    ns1:author "Dominique de Saint Mars" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=25796" ;
