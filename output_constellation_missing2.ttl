ns1:Bookd78a40e7-59d9-4c0b-ab7e-fba391cf51cf a ns1:Book ;
    ns1:author "Maria Isabel Sánchez Vegara" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=54951" ;
