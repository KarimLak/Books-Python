ns1:Book2592e060-e228-4dc8-8a0c-007fc263238f a ns1:Book ;
    ns1:author "Yves Sciama" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=42129" ;
