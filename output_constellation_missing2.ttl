ns1:Bookad2b68b6-b08c-4fb7-befa-7ec60584350b a ns1:Book ;
    ns1:author "Kathrin Schärer" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49279" ;
    ns1:datePublished "2015"^^xsd:gYear ;
