ns1:Book4711831c-ce06-4491-a4f3-6b8e28786336 a ns1:Book ;
    ns1:author "Christian Georges Schwentzel" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45063" ;
    ns1:datePublished "2002"^^xsd:gYear ;
