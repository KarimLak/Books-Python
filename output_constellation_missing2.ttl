ns1:Book8c80914c-9b0c-4b3e-8201-224e0f50a500 a ns1:Book ;
    ns1:author "Susumu Shingu" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51391" ;
