ns1:Bookc94fc782-bd14-4d4e-a2e3-c63a09ccc322 a ns1:Book ;
    ns1:author "Craig Silvey" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50896" ;
