ns1:Book8b884f68-0a7e-4ad5-b28e-8446f76c6601 a ns1:Book ;
    ns1:author "Lola M. Schaefer" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47758" ;
    ns1:datePublished "2012"^^xsd:gYear ;
