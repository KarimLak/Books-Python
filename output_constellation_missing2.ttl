ns1:Bookbb31e464-3962-4c45-90e0-1da54d584b77 a ns1:Book ;
    ns1:author "Robert Louis Stevenson" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49107" ;
    ns1:datePublished "1883"^^xsd:gYear ;
