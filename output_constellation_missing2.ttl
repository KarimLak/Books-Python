ns1:Book5bce6f1a-25e5-4a6e-ba2a-d738b52795a1 a ns1:Book ;
    ns1:author "Caroline Stills" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=55942" ;
    ns1:datePublished "2021"^^xsd:gYear ;
