ns1:Book6a4d7aa8-6951-42b4-953e-cfb078dc1a69 a ns1:Book ;
    ns1:author "Susan Steggall" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49640" ;
