ns1:Book1efaf052-2a79-4360-890d-c254148450fb a ns1:Book ;
    ns1:author "Francesca Sanna" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49699" ;
