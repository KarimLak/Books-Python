ns1:Book50ed2fe7-5ba4-43ac-a617-76ce66eee592 a ns1:Book ;
    ns1:author "Grégoire Solotareff" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49494" ;
