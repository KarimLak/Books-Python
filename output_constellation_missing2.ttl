ns1:Book6f7d2681-8683-4b94-a89d-27c8f7c08049 a ns1:Book ;
    ns1:author "Thomas Scotto" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49000" ;
