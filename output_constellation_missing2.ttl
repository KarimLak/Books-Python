ns1:Book86908170-ed18-4975-a5ac-5366e59a5b64 a ns1:Book ;
    ns1:author "Aline Sax" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50927" ;
