ns1:Book4736f6af-a19b-4540-92f2-d1880d752bec a ns1:Book ;
    ns1:author "Joel Franz Rosell" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=41625" ;
    ns1:datePublished "2002"^^xsd:gYear ;
