ns1:Booke99206d9-ffce-4f83-b4ff-ad720659cb75 a ns1:Book ;
    ns1:author "Sara O'Leary" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51170" ;
    ns1:datePublished "2016"^^xsd:gYear ;
