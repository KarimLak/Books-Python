ns1:Book6136df14-eac8-4129-a1ed-ed0ad3d91e1c a ns1:Book ;
    ns1:author "Amy Novesky" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49735" ;
