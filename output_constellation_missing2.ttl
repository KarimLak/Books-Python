ns1:Book6ed957e3-1d94-4f8b-b50b-d384d4204001 a ns1:Book ;
    ns1:author "Angela Royston" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50179" ;
