ns1:Book3726065b-ed9d-4044-b900-dd198f22d67e a ns1:Book ;
    ns1:author "Christian Oster" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=37072" ;
