ns1:Book4cf43739-b057-4615-bdbc-091fd3ccf1eb a ns1:Book ;
    ns1:author "Philip Steele" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=28000" ;
