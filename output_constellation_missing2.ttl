ns1:Bookf7799a65-22ad-4b86-9f32-c56db6c29253 a ns1:Book ;
    ns1:author "Manuela Salvi" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=41594" ;
