ns1:Bookd4d91ba2-077c-4734-938e-ee9127d68881 a ns1:Book ;
    ns1:author "Brian Selznick" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46407" ;
