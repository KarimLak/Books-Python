ns1:Book07724f23-020e-4867-b681-e58d63154235 a ns1:Book ;
    ns1:author "Carole Saturno" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48131" ;
    ns1:datePublished "2013"^^xsd:gYear ;
