ns1:Book48fd99c7-246a-48e6-95cf-a4a2a3784c05 a ns1:Book ;
    ns1:author "Quitterie Simon" ;
    pbs:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=44422" ;
