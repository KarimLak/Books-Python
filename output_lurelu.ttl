    ns1:name "La boîte aux lettres" .

ns1:Book34552118 a ns1:Book ;
    ns1:author "Terry",
      "Eric Fan" ;
    ns1:lureluLink "https://www.lurelu.net/coupsdecoeur06.html#45_2_2022" ;
    ns1:datePublished "2022"^^xsd:gYear ;
    ns1:language "Français" ;
    ns1:publisher ns1:Scholastic ;
    ns1:review "C’est beaucoup en tant qu’adulte que j’ai pris plaisir à cette représentation de l’univers de l’enfance et de cette mini société. (R. L.)" ;
