    ns1:language "Français" ;
    ns1:publisher ns1:Scholastic ;
    ns1:review "En littérature jeunesse, il y aurait donc encore de la place pour le plaisir, le simple plaisir des yeux, sans thèse et sans manifeste. (D. S.)" ;
    ns1:name "Sur ma route" .
   
ns1:Book34552121 a ns1:Book ;
    ns1:author "Rogé" ;
    ns1:lureluLink "https://www.lurelu.net/coupsdecoeur05.html" ;
