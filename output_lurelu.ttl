    ns1:datePublished "2013"^^xsd:gYear ;
    ns1:language "Français" ;
    ns1:publisher ns1:La_Courte_Échelle ;
    ns1:review "Ah, l'humour intelligent! Si délicieux à lire et malheureusement trop rare en littérature jeunesse. (AP)" ;
    ns1:name "Les maux d'Ambroise Bukowski" .

ns1:Book34562145 a ns1:Book ;
