ns1:Book34552139 a ns1:Book ;
    ns1:author "Pierre Lapointe" ;
    ns1:lureluLink "https://www.lurelu.net/coupsdecoeur05.html" ;
    ns1:eruditLink "https://www.erudit.org/fr/revues/lurelu/2016-v39-n2-lurelu02619/82863ac/" ;
    ns1:illustrator "Catherine Lepage" ;
    ns1:datePublished "2016"^^xsd:gYear ;
    ns1:language "Français" ;
    ns1:publisher ns1:Comme_Des_Géants ;
    ns1:review "Peu d’albums créent de l’émoi, bousculent les repères, provoquent des discussions autant que Pépito. On a adoré, souvent, on a moins aimé peut-être, mais on en a parlé. (M.F.)" ;
    ns1:name "Le tragique destin de Pépito" .

ns1:Book34552140 a ns1:Book ;
    ns1:author "Christine Baldacchino" ;
    ns1:lureluLink "https://www.lurelu.net/coupsdecoeur05.html" ;
