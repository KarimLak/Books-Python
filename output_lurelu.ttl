    ns1:name "Le petit chevalier qui n'aimait pas la pluie" .

ns1:Book34552125 a ns1:Book ;
    ns1:author "Susin Nielsen" ;
    ns1:lureluLink "https://www.lurelu.net/coupsdecoeur05.html" ;
