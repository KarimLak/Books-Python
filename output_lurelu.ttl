    ns1:datePublished "2012"^^xsd:gYear ;
    ns1:language "Français" ;
    ns1:publisher ns1:La_Pastèque ;
    ns1:review "J'aime les livres qui donnent le goût de relire les grands auteurs et l'occasion de les faire découvrir aux enfants. (MR)" ;
    ns1:name "Virginia Wolf" .

ns1:Book04563145 a ns1:Book ;
