    ns1:language "Français" ;
    ns1:publisher ns1:Soulières ;
    ns1:review "Ce sont les mots, la manière, la poésie de Pierre Labrie dans Nous sommes ce continent, un livre sur l'adolescence comme il y en a peu, qui ont supplanté toutes mes autres lectures de 2012. (MF)" ;
    ns1:name "Nous sommes ce continent" .

ns1:Book34552124 a ns1:Book ;
    ns1:author "Gilles Tibo" ;
    ns1:lureluLink "https://www.lurelu.net/coupsdecoeur05.html" ;
    ns1:eruditLink "https://www.erudit.org/fr/revues/lurelu/2013-v36-n2-lurelu0810/69855ac/" ;
