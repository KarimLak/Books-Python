    ns1:publisher ns1:La_Pastèque ;
    ns1:review "Ces pages de pur plaisir dans le style streamline ou art déco, en beige et brun. (D. S.)" ;
    ns1:name "En voiture !" .

ns1:Book34552155 a ns1:Book ;
    ns1:author "Eric Fan",
        "Terry Fan" ;
    ns1:lureluLink "https://www.lurelu.net/coupsdecoeur05.html" ;
