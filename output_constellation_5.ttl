ns1:Book88a4ec0a-6c89-4358-8b32-d0a9313b9824 a ns1:Book ;
    ns1:author "Stian Hole" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48201" ;
