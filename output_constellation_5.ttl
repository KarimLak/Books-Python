ns1:Book7d8017fb-a5d4-4b03-9316-c703485d7acd a ns1:Book ;
    ns1:author "Dick King-Smith" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=14079" ;
    ns1:datePublished "1981"^^xsd:gYear ;
