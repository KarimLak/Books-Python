ns1:Book316a3241-725e-415e-85a1-2ce3519a5875 a ns1:Book ;
    ns1:author "Bobbie Kalman" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46486" ;
