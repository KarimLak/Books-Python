ns1:Book9647118e-89a5-4705-9d91-85420a5e622b a ns1:Book ;
    ns1:author "Petr Horacek" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46365" ;
    ns1:datePublished "2012"^^xsd:gYear ;
