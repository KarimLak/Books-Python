ns1:Bookced58ace-3b1a-487a-9e31-4bb67b399f75 a ns1:Book ;
    ns1:author "Johanne Jarry" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=55621" ;
