ns1:Bookf0c60958-1ecb-45a3-9bd4-51de752fbfd7 a ns1:Book ;
    ns1:author "Érik L'Homme" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=25799" ;
