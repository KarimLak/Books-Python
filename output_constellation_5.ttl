ns1:Book91b9b5f3-0fb6-4407-8d03-9f4a8c7a425f a ns1:Book ;
    ns1:author "Margaret Christy et Pokiak-Fenton Jordan-Fenton" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=54749" ;
