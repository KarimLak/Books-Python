ns1:Bookba809b40-22ab-4167-8635-43b8185b7ba4 a ns1:Book ;
    ns1:author "Amy Krouse Rosenthal" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47600" ;
