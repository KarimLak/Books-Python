ns1:Book1a8534d3-5ca8-441b-afdf-2b255df4cd0d a ns1:Book ;
    ns1:author "Sharon Jennings" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=14364" ;
