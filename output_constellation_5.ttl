ns1:Book80b8ea75-e0e7-4425-a999-969287079135 a ns1:Book ;
    ns1:author "Mari Kasai" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50168" ;
