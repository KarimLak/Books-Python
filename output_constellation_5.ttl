ns1:Book39ac0f68-4cbc-42ee-9e26-284f36aed3aa a ns1:Book ;
    ns1:author "Michel Brigitte et Puech Labbé" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=39686" ;
