ns1:Book1fc1274b-1f7e-4dc0-947f-af81d03bafbc a ns1:Book ;
    ns1:author "Rob Hodgson" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51052" ;
    ns1:datePublished "2017"^^xsd:gYear ;
