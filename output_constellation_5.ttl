ns1:Bookff42a81c-4172-4aca-8215-dc83ef41ce6e a ns1:Book ;
    ns1:author "Gérard de La Taille" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50217" ;
