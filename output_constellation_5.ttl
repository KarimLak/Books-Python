ns1:Book7efe74ad-3df3-407c-b5f0-71505c86d162 a ns1:Book ;
    ns1:author "Masako Izawa" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=42015" ;
