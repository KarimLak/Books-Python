ns1:Book8f70324c-ef6c-43ae-880c-3f56f57c30ec a ns1:Book ;
    ns1:author "Kate Klise" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=54833" ;
