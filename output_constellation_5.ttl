ns1:Booka07612e6-022e-469c-9a10-af9b0aa8c413 a ns1:Book ;
    ns1:author "Margaret Hodges" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48722" ;
    ns1:datePublished "1964"^^xsd:gYear ;
