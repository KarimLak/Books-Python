ns1:Bookc53587fa-53ab-4c82-a1e4-2d8347c7476b a ns1:Book ;
    ns1:author "Émile Jadoul" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49236" ;
