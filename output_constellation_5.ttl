ns1:Book87aae2f2-536d-4aad-89f0-39fc81945033 a ns1:Book ;
    ns1:author "Caroline Laffon" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47720" ;
    ns1:datePublished "2013"^^xsd:gYear ;
