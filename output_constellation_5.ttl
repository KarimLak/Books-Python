ns1:Booke9a4dc6b-637f-461b-877c-c657e29bde14 a ns1:Book ;
    ns1:author "Pierre-François Brigitte et Dupont-Beurier Labbé" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=41814" ;
