ns1:Book6c4ee3f7-c3e1-43de-9f5a-df29162efc5b a ns1:Book ;
    ns1:author "Herman Jean-Pierre d'après Melville Kerloc'h" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=27381" ;
