ns1:Booka1dbb504-b7dc-4626-8120-6f268472f090 a ns1:Book ;
    ns1:author "Donald Brien Johnson" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=38244" ;
    ns1:datePublished "2000"^^xsd:gYear ;
