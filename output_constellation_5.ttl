ns1:Booke2267321-2aa7-4f5d-b7af-5bd77af05c02 a ns1:Book ;
    ns1:author "Anna Kang" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48418" ;
