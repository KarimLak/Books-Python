ns1:Book7c14d3f9-72b2-4dd0-a239-1d1adef75523 a ns1:Book ;
    ns1:author "Marie Houblon" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45739" ;
