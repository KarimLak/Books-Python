ns1:Book8f10988d-e570-419c-b0e3-73769500ab98 a ns1:Book ;
    ns1:author "Jo Hoestlandt" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=35426" ;
