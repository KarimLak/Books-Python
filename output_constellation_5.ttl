ns1:Bookce297ac3-fa24-49ac-8072-af6ad802e750 a ns1:Book ;
    ns1:author "Rudyard Kipling" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=40314" ;
