ns1:Bookb8e55142-6d6d-4d55-bdad-707db39ee1c1 a ns1:Book ;
    ns1:author "Jim Kerr" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=39365" ;
