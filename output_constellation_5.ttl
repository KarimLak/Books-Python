ns1:Book1d2bdc3a-13fd-4224-99ae-91963df679cd a ns1:Book ;
    ns1:author "Nathalie Kuperman" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=35327" ;
