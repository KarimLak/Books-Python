ns1:Book4d029c56-bdfc-409b-a3ef-da14f748f756 a ns1:Book ;
    ns1:author "Josh Lacey" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=42013" ;
