ns1:Book0cddd678-9649-4f21-a5f6-9f518db2682d a ns1:Book ;
    ns1:author "L. Frank d'après Baum Jean-Pierre Kerloc'h" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48682" ;
    ns1:datePublished "1900"^^xsd:gYear ;
