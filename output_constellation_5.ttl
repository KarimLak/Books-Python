ns1:Bookb8fac31e-e2b6-438a-9029-07180e3ac68b a ns1:Book ;
    ns1:author "James Houston" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=14902" ;
    ns1:datePublished "1968"^^xsd:gYear ;
