ns1:Book6eeddcee-ab94-4983-bff0-70a8d71b9dc7 a ns1:Book ;
    ns1:author "Émile Jadoul" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=33665" ;
