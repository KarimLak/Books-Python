ns1:Book81dcf9f4-1d95-4d3a-9837-a1d1386544bd a ns1:Book ;
    ns1:author "Dany Laferrière" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48001" ;
