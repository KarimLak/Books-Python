ns1:Bookd3b979f9-2677-4c13-b376-99f91b4d6160 a ns1:Book ;
    ns1:author "Tove Jansson" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46779" ;
