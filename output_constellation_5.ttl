ns1:Bookff761ae3-4ae8-4646-bd37-227bcfc1b8bc a ns1:Book ;
    ns1:author "Michel Brigitte et Puech Labbé" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=37583" ;
