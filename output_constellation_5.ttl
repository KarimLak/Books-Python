ns1:Book73a5f5be-a1d4-42c9-933b-9ea868386ad4 a ns1:Book ;
    ns1:author "Rudyard Kipling" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=39044" ;
