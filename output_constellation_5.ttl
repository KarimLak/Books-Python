ns1:Bookf93b5278-b0c2-481c-864c-4e5cf406b8bb a ns1:Book ;
    ns1:author "Caroline Laffon" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49644" ;
    ns1:datePublished "2016"^^xsd:gYear ;
