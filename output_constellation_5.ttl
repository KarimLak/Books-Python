ns1:Bookb777709f-8ea0-4158-b5d1-2f30634b5460 a ns1:Book ;
    ns1:author "Jean de La Fontaine" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48399" ;
