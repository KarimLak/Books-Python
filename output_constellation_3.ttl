ns2:Book737aa422-d037-4446-ba8c-638d6505661e a ns2:Book ;
    ns2:author "Zoran Drvenkar" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=40739" ;
