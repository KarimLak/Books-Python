ns2:Book311c28fc-3385-4d45-94ac-76d912da3dcc a ns2:Book ;
    ns2:author "Sylvie Dodeller" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50005" ;
    ns2:datePublished "2010"^^xsd:gYear ;
