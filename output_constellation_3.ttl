ns2:Bookacd443b5-a581-4daf-a8f8-00d1627e7c8d a ns2:Book ;
    ns2:author "Jenni Desmond" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50689" ;
