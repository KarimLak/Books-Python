ns2:Book3c157ab2-65dd-4e93-86a3-4be45e3d0fad a ns2:Book ;
    ns2:author "Elina Ellis" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=54668" ;
    ns2:datePublished "2019"^^xsd:gYear ;
