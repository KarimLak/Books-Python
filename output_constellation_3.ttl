ns2:Book21318742-3806-479f-abe7-d1f9771cc055 a ns2:Book ;
    ns2:author "Frank et Bom" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=40328" ;
