ns2:Bookad10406c-bffc-4cda-9a3d-024f5d9888be a ns2:Book ;
    ns2:author "John Farndon" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=38518" ;
