ns2:Bookea286ca5-9ce4-4a30-80bc-597b4ccc3c3e a ns2:Book ;
    ns2:author "Stéphane Frattini" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=41022" ;
