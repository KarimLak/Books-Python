ns2:Book73b2ddab-946d-439b-bb77-f911f475c66b a ns2:Book ;
    ns2:author "Heather Dyer" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=27335" ;
