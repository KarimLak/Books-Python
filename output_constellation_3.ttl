ns2:Book175bac55-7928-456d-aa87-325316353f35 a ns2:Book ;
    ns2:author "Dominique Géraldine d'après Marchand Elschner" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45516" ;
