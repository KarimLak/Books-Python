ns2:Book6b042faa-fcea-4b95-ba1b-8ec89084082f a ns2:Book ;
    ns2:author "Julie Flett" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=56993" ;
