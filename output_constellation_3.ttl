ns2:Book3316c22e-ec85-4442-972f-29bee7e3f776 a ns2:Book ;
    ns2:author "Cornelia Funke" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=27379" ;
