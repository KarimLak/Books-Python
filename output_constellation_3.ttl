ns2:Bookd9869719-3f53-4f3c-9d98-eb6baa0dba77 a ns2:Book ;
    ns2:author "John Meade Falkner" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=44233" ;
