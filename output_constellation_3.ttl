ns2:Book14115ccb-7373-49ba-aee8-82e55834475e a ns2:Book ;
    ns2:author "Elsa Devernois" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51110" ;
