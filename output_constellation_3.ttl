ns2:Book2b96e10d-74d3-4f93-8bc0-6433f5ffbc47 a ns2:Book ;
    ns2:author "Béatrice Fontanel" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=27564" ;
    ns2:datePublished "2004"^^xsd:gYear ;
