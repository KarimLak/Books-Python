ns2:Book653b44c4-dece-41fb-864b-7146333e5fb9 a ns2:Book ;
    ns2:author "Caroline Desnoëttes" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=36466" ;
