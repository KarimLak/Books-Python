ns1:Book727ad4f4-37e7-433e-bc46-c2f47d2de0d8 a ns1:Book ;
    ns1:author "Thierry Dedieu" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=37963" ;
