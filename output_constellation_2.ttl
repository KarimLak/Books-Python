ns1:Book21f071a6-1789-477d-8ab2-83abf119f948 a ns1:Book ;
    ns1:author "Hector Yann d'après Malot Dégruel" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=34757" ;
