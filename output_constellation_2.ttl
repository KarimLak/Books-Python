ns1:Bookfa4040f3-8064-4c86-9caa-39272ef24ce9 a ns1:Book ;
    ns1:author "Roald Dahl" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51420" ;
