ns1:Bookc0e6c512-0cd1-43f8-a3e6-167e36f542a1 a ns1:Book ;
    ns1:author "Collectif" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45052" ;
    ns1:datePublished "2011"^^xsd:gYear ;
