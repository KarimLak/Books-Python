ns1:Book9d75f214-7485-4095-8719-00254a414183 a ns1:Book ;
    ns1:author "Pierre Chastenay" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=20920" ;
