ns1:Bookb109a3b8-6f7c-4cec-8a05-59521ba69100 a ns1:Book ;
    ns1:author "Gérard Coulon" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50061" ;
