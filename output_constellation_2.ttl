ns1:Bookc97c7d77-a920-450a-b34e-e032fcc49cde a ns1:Book ;
    ns1:author "John Cleare" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47415" ;
