ns1:Bookf104c4fa-66af-46b5-876f-e62b54a13c9d a ns1:Book ;
    ns1:author "Iwona Chmielewska" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50313" ;
