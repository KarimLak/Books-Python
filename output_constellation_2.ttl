ns1:Bookc5084713-2522-4250-9efa-92370ddf6ecc a ns1:Book ;
    ns1:author "Giulia De Amicis" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51115" ;
