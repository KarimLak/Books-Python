ns1:Book475c1c9d-4a80-4894-bf23-85925a204fd8 a ns1:Book ;
    ns1:author "Simona Ciraolo" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49663" ;
