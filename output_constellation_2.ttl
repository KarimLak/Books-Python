ns1:Bookf1dc8774-23a6-4341-b560-f0b7269c35be a ns1:Book ;
    ns1:author "Emma Chichester Clark" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=33663" ;
    ns1:datePublished "2005"^^xsd:gYear ;
