ns1:Bookc4b7f259-878d-4117-8aab-839ada2c79ee a ns1:Book ;
    ns1:author "Philippe Corentin" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=12334" ;
