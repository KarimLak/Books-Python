ns1:Booka5cdd0ad-6074-49ba-836b-5307dc28bbdd a ns1:Book ;
    ns1:author "Clothilde Delacroix" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50697" ;
