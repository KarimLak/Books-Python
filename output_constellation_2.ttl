ns1:Book8d3f36e4-fd33-48ec-8d79-b1b49dd8dc6e a ns1:Book ;
    ns1:author "Lesa Cline-Ransome" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=53784" ;
