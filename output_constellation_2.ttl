ns1:Book483f8a34-3859-45c4-852f-22b059a15be5 a ns1:Book ;
    ns1:author "Benjamin Chaud" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=43856" ;
