ns1:Book186a52cf-472d-40a6-a06b-6dd57aa33114 a ns1:Book ;
    ns1:author "Collectif" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=35392" ;
