ns1:Bookbbfee40e-308d-49a8-93de-0cdbb2e439e6 a ns1:Book ;
    ns1:author "Collectif" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47419" ;
