ns1:Book1ed4e8a3-ff58-4124-8d40-60f29c81fbe0 a ns1:Book ;
    ns1:author "Gauthier David" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51208" ;
