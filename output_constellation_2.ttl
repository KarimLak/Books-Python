ns1:Book0eecd717-26c2-4b15-8e2b-dc63a76187c9 a ns1:Book ;
    ns1:author "Carlo Collodi" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=33837" ;
