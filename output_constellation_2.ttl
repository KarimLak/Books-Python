ns1:Book5843cdb9-fb3c-4912-aa02-ce0807c61cac a ns1:Book ;
    ns1:author "Collectif" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51286" ;
