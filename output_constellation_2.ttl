ns1:Bookd78a5b4e-cc0f-4c8e-807c-6ed913403e71 a ns1:Book ;
    ns1:author "Roald Dahl" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49871" ;
