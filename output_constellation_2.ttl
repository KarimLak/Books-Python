ns1:Bookd32d18a4-5775-4170-a113-c51ab7533ea6 a ns1:Book ;
    ns1:author "Anna Claybourne" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49924" ;
