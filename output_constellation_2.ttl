ns1:Bookfff50b35-470f-4ce9-8b66-1394c9b88206 a ns1:Book ;
    ns1:author "Louis Dantin" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=26866" ;
