ns1:Book41d05287-b537-4457-946b-7d917ca42c25 a ns1:Book ;
    ns1:author "Carle Coppens" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49346" ;
