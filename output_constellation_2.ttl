ns1:Book5b80f363-a714-4082-9097-4d34fa573d6e a ns1:Book ;
    ns1:author "Danielle Choquette" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=37978" ;
