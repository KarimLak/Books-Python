ns1:Book7efc0824-1bed-41f5-9695-ba0c0371d2c5 a ns1:Book ;
    ns1:author "Collectif" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45325" ;
