ns1:Book819f8fee-e193-42da-8ff9-f8a1f306fc3d a ns1:Book ;
    ns1:author "Ariane Jean-Claude et Archambault Corbeil" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=41045" ;
