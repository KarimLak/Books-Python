ns1:Book9e3dd569-bab2-4d12-8136-d26e18c735dd a ns1:Book ;
    ns1:author "Roald Dahl" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51425" ;
