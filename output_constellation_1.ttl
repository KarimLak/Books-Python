ns1:Book74e8d7b1-7f6e-4fa8-b941-40bb1cb6150b a ns1:Book ;
    ns1:author "Caroline Bingham" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=38779" ;
