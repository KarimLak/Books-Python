ns1:Bookcd71d1d3-48d6-4bb0-9b15-1aa99dbf9fa4 a ns1:Book ;
    ns1:author "Oscar Brenifier" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46686" ;
