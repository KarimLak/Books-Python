ns1:Book57a10928-d6b6-45a8-92c1-bfa02f1bc9b1 a ns1:Book ;
    ns1:author "Sandrine Marianne et Mirza Boilève" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45406" ;
