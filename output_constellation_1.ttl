ns1:Book1431f2f8-95fe-4800-b1d1-868e667d96c8 a ns1:Book ;
    ns1:author "Quentin Blake" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=41266" ;
