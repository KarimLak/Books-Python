ns1:Book796efce4-42d9-4774-a256-e235e394512e a ns1:Book ;
    ns1:author "Linda Calabresi" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45551" ;
