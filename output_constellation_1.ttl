ns1:Bookd334ed75-31c0-4dec-a521-eea1766c134a a ns1:Book ;
    ns1:author "Douglas Charing" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50554" ;
