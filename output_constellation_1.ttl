ns1:Bookb0ed3ab2-748d-4c10-904c-6c0f64d7ecc7 a ns1:Book ;
    ns1:author "Davide Calì" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=39899" ;
