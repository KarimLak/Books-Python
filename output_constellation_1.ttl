ns1:Bookf55290ad-2f21-473d-8e1d-015084029ef9 a ns1:Book ;
    ns1:author "Roch Carrier" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=10460" ;
