ns1:Bookadb35ef5-4bed-45aa-b8af-95a6c889e05a a ns1:Book ;
    ns1:author "Laura Buller" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=36462" ;
