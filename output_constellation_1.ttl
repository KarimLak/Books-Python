ns1:Bookf92cc657-8acb-45b6-82bd-5ca6d0451c7a a ns1:Book ;
    ns1:author "André Bouchard" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51363" ;
