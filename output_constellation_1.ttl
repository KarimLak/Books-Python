ns1:Bookcc290246-5171-4f3f-a358-9e0b72625cb1 a ns1:Book ;
    ns1:author "Felicity Brooks" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51578" ;
