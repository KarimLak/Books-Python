ns1:Bookfed1719f-d50d-4ca8-a23d-4ff4e213a70c a ns1:Book ;
    ns1:author "Eric Carle" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=40776" ;
    ns1:datePublished "1984"^^xsd:gYear ;
