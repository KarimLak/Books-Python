ns1:Book0da03cb4-8c25-41e5-8eb1-f0c90942cd80 a ns1:Book ;
    ns1:author "Frances H. Burnett" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=45589" ;
    ns1:datePublished "1886"^^xsd:gYear ;
