ns1:Booke1705a6d-372a-4c4b-a383-35924a290325 a ns1:Book ;
    ns1:author "Claude Boujon" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=33974" ;
