ns1:Book26acc332-721a-48c3-91e9-edb456ecbec6 a ns1:Book ;
    ns1:author "Catherine Chambers" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46455" ;
