ns1:Book06875f85-21da-4576-88b4-492652be1815 a ns1:Book ;
    ns1:author "David Burnie" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=39345" ;
    ns1:datePublished "1988"^^xsd:gYear ;
