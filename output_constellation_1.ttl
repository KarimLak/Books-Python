ns1:Book19e82820-c50e-4f9d-8eb2-a6b039d62649 a ns1:Book ;
    ns1:author "Fred Bernard" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=27934" ;
