ns1:Book894904bb-a1bf-4088-a2ff-bf339f0dadc1 a ns1:Book ;
    ns1:author "Camille Bouchard" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=37969" ;
