ns1:Bookff8bf55c-0424-4e80-a681-4b2e9f245069 a ns1:Book ;
    ns1:author "David Burnie" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=34443" ;
