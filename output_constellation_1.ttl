ns1:Book8cd2b05e-eba1-43fa-bd34-f4595bcbc80d a ns1:Book ;
    ns1:author "Jennifer Berne" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51001" ;
