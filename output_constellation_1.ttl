ns1:Book69284cc2-33db-413f-a0fc-026cb83f15fc a ns1:Book ;
    ns1:author "Anthony Browne" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48543" ;
    ns1:datePublished "2014"^^xsd:gYear ;
