ns1:Book1bd96b94-ef48-4148-9c82-b80a78c908a1 a ns1:Book ;
    ns1:author "Jan Brett" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=38374" ;
