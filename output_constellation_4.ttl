ns1:Book19a45458-80a9-4f8e-91ee-f41f72bf0753 a ns1:Book ;
    ns1:author "Agnès Grimaud" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=39595" ;
    ns1:datePublished "2008"^^xsd:gYear ;
