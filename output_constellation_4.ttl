ns1:Bookbf329ae5-ec52-4b1f-940a-176d38132db2 a ns1:Book ;
    ns1:author "François Gravel" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51312" ;
