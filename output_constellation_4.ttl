ns1:Book4add364c-61de-4f2c-a135-8d4a50c89b6f a ns1:Book ;
    ns1:author "Jacob et Wilhelm Grimm" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=39921" ;
    ns1:datePublished "1814"^^xsd:gYear ;
