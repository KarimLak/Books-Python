ns1:Bookc1607582-cde3-433e-b79d-0c0942840772 a ns1:Book ;
    ns1:author "Marie-Odile d'après Homère Hartmann" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=25910" ;
