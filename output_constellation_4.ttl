ns1:Bookd561c621-e1fd-4574-a4ad-51ce4903c636 a ns1:Book ;
    ns1:author "Von Hardesty" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46043" ;
