ns1:Book598cbe84-57f3-4350-9ee7-5b5016759ce4 a ns1:Book ;
    ns1:author "Ian Graham" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46978" ;
