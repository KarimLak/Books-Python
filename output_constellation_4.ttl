ns1:Book228a80d3-e380-4a36-909d-e2eed573a572 a ns1:Book ;
    ns1:author "Don Gillmor" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=37605" ;
