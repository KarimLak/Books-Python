ns1:Book2c339998-a9e0-4b74-ae58-f9eb3db98119 a ns1:Book ;
    ns1:author "François Gravel" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=18969" ;
