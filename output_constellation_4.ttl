ns1:Bookb691e2b1-2d09-496a-913e-85b955e70952 a ns1:Book ;
    ns1:author "Marie-Francine Hébert" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50813" ;
