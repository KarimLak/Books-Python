ns1:Book345243cd-2053-4f42-b4cf-48e0c1ed1e4c a ns1:Book ;
    ns1:author "Jacob et Wilhelm Grimm" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=25890" ;
