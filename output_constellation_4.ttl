ns1:Bookf73ab319-11b4-44ac-9df8-b9be1f208a43 a ns1:Book ;
    ns1:author "Annemarie van Haeringen" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=49519" ;
