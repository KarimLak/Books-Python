ns1:Booka8f3742c-279c-4b00-b36a-b0db3ff8fb62 a ns1:Book ;
    ns1:author "Neil Gaiman" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=36239" ;
    ns1:datePublished "1999"^^xsd:gYear ;
