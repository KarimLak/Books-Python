ns1:Bookf38700db-3110-4999-aae3-deebdcba00e8 a ns1:Book ;
    ns1:author "Pierre Gripari" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=33620" ;
    ns1:datePublished "1967"^^xsd:gYear ;
