ns1:Bookddea48e3-1ef0-4845-8490-c73496989941 a ns1:Book ;
    ns1:author "Élise Gravel" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46379" ;
