ns1:Book40b3c495-57e6-40ef-a08c-9bde07786083 a ns1:Book ;
    ns1:author "Jamila Gavin" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=44385" ;
    ns1:datePublished "2000"^^xsd:gYear ;
