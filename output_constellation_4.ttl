ns1:Booke4cfbdb3-b12e-4da5-9db0-57dd4e6d15bf a ns1:Book ;
    ns1:author "Peter van Gestel" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=44271" ;
    ns1:datePublished "1997"^^xsd:gYear ;
