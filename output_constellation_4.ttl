ns1:Book5f4d39b0-0b50-4642-a40e-60cd245e92ea a ns1:Book ;
    ns1:author "Chris Hadfield" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50064" ;
