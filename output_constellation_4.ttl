ns1:Book06fa14f3-f0a1-43b7-8d7d-7b4072b913e2 a ns1:Book ;
    ns1:author "Ariane Goetz" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=46761" ;
    ns1:datePublished "2012"^^xsd:gYear ;
