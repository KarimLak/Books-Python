ns1:Bookc4e3010b-c379-4e0b-a9db-1349919189c2 a ns1:Book ;
    ns1:author "Betty Hagglund" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=47417" ;
