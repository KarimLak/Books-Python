ns1:Book4d3f16de-97cb-4572-ad25-76f728d14983 a ns1:Book ;
    ns1:author "Gabrielle Grimard" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50821" ;
