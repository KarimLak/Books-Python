ns1:Book14a4d8db-ab52-49ff-bdae-894863b09832 a ns1:Book ;
    ns1:author "Wilhelm Hauff" ;
    ns1:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=48204" ;
    ns1:datePublished "1826"^^xsd:gYear ;
