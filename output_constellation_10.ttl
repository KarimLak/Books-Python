ns2:Book03cfa4a4-9a1b-441a-99d9-dc8ae4510ff7 a ns2:Book ;
    ns2:author "John Woodward" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=40076" ;
