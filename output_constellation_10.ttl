ns2:Book965b61f5-66bb-49df-b3fb-b29f5ba25e83 a ns2:Book ;
    ns2:author "Margaret Wise Brown" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=37780" ;
    ns2:datePublished "1942"^^xsd:gYear ;
