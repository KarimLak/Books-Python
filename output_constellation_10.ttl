ns2:Book96033474-4b3c-4844-b5d7-21a97f90c659 a ns2:Book ;
    ns2:author "John Yeoman" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=51206" ;
    ns2:datePublished "1995"^^xsd:gYear ;
