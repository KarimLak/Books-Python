ns2:Book0e0f8dbd-cacb-4f13-be35-175a45430f1e a ns2:Book ;
    ns2:author "John Yeoman" ;
    ns2:constellationLink "https://constellations.education.gouv.qc.ca/index.php?p=il&lo=50626" ;
